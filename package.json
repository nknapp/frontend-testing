{
  "name": "frontend-testing-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "",
  "scripts": {
    "dev:preview": "vitest-preview",
    "dev:server": "vite",
    "dev:unit": "vitest --ui",
    "dev:e2e": "playwright test --ui",
    "dev": "run-p dev:*",
    "test:lint": "eslint .",
    "test:ts": "vue-tsc",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test": "run-s test:*",
    "build": "vue-tsc && vite build",
    "format": "prettier -w .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "vue": "^3.5.8",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@playwright/test": "1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "22.15.2",
    "@types/ramda": "^0.30.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "cross-fetch": "4.0.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-vue": "9.28.0",
    "globals": "15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "msw": "2.4.9",
    "nanoid": "^5.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ramda": "^0.30.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "8.7.0",
    "vite": "^5.4.7",
    "vitest": "^2.1.1",
    "vitest-preview": "^0.0.1",
    "vue-tsc": "^2.1.6"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --max-warnings=0 --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
